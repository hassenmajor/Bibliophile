<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="https://jakarta.ee/xml/ns/jakartaee" 
	id="projet.liu.bibliophile" 
	version="6.0">
  <display-name>Bibliophile</display-name>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <display-name>Home</display-name>
    <servlet-name>Home</servlet-name>
    <servlet-class>HomeServlet</servlet-class>
  </servlet>
  
  <servlet>
    <display-name>Livre</display-name>
    <servlet-name>Livre</servlet-name>
    <servlet-class>LivreServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>Livre</servlet-name>
    <url-pattern>/livre</url-pattern>
  </servlet-mapping>
  
</web-app>
